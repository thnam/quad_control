#faultHistory.tab-pane.container.fade(role='tabpanel')
  #container(style='width:100%;')
    .row.mt-4
      .col-4
      .col-4
        button#btnRefreshFaultHistory.btn.btn-success Refresh Table
      .col-4
    .row.mt-4
      .table-responsive
        table.table.table-sm.table-striped.table-hover.table-bordered#faultHistoryTable
          - const pulsers = ["NTS", "NOS", "POS", "PTS"]
          thead
            tr
              th(width='16%',data-field='timestamp') Timestamp
              each p in pulsers
                th(width='21%')= p
          tbody#faultHistoryTableBody
