#sparkHistory.tab-pane.container.fade(role='tabpanel')
  #container(style='width:100%;')
    .row.mt-4
      h4.center Last 300 sparks
    .row.mt-4
      .col-4
        |Number of entries: 
      .col-4
        button#btnRefreshSparkHistory.btn.btn-success Refresh Table
      .col-4
    .row.mt-4
      .table-responsive
        table.table.table-hover.table-bordered#sparkHistoryTable
          tr
            th(width='15%') Timestamp
            th(width='75%') Spark pattern
            th(width='10%') Spark bit

